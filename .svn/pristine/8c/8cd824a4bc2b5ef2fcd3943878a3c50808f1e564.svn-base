
@font-face {
  font-family: 'iconfont';  /* project id 942614 */
  src: url('//at.alicdn.com/t/font_942614_rcel0rwl11n.eot');
  src: url('//at.alicdn.com/t/font_942614_rcel0rwl11n.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_942614_rcel0rwl11n.woff') format('woff'),
  url('//at.alicdn.com/t/font_942614_rcel0rwl11n.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_942614_rcel0rwl11n.svg#iconfont') format('svg');
}

.iconfont {

  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  /*字体清除锯齿效果*/
  -webkit-text-stroke-width: 0.2px;
  /*文本外边框大小*/
  -moz-osx-font-smoothing: grayscale;
}
